<body>


<table id="Tabelka" style="align: center;">


  <tr>
    <td>Tabliczka</td>
    <td>Oskar H</td>
  </tr>


</table>
<br>
<button type="button" onclick="klik()">Dodaj </button>

<script>
function klik() {
 
  var tabela = document.getElementById("Tabelka");
  for (i=0; i<=10; i++)
  {
  var row = tabela.insertRow(0);
  var cell1 = row.insertCell(0);
  var cell2 = row.insertCell(1);    

  var numer = (0,i);

 
  cell1.innerHTML = "7*"+i+"="+( numer*7);
  cell2.innerHTML = "Oskar H";
  }

}
</script>

